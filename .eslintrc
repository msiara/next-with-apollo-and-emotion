{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": ["error", { "allow": ["error"] }],
    "react/destructuring-assignment": ["on", "always", { "ignoreClassFields": true }],
    "react/jsx-filename-extension": "off",
    "react/no-danger": "off"
  }
}
